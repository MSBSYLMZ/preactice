    "micro-cors": "^0.1.1",
    "next": "12.2.5",
    "next-auth": "^4.10.3",
    "nexus": "^1.3.0",
    "nexus-validate": "^1.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
